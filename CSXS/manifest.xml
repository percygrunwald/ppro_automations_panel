<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="5.0" ExtensionBundleId="com.percygrunwald.PPROAutomationsPanel" ExtensionBundleVersion="11.1" ExtensionBundleName="Premiere Pro sample panel"
